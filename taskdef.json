{
    "family": "swan-migration-task-dev",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::570489932204:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "swan-migration-container",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "development"
                },
                {
                    "name": "PORT",
                    "value": "3000"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/swan-migration-dev",
                    "awslogs-region": "eu-north-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}